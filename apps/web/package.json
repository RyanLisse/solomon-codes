{
	"name": "web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack --port=3001",
		"build": "next build",
		"build:original": "next build",
		"build:bypass": "node scripts/bypass-build-error.js disable && next build && node scripts/bypass-build-error.js restore",
		"build:production": "NODE_ENV=production next build",
		"build:analyze": "ANALYZE=true next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"check": "tsc --noEmit",
		"check-types": "tsc --noEmit",
		"typecheck": "tsc --noEmit",
		"test": "bun run test:unit",
		"test:unit": "bun run test:unit:logic && bun run test:unit:components",
		"test:unit:logic": "bun test scripts/",
		"test:unit:logic:watch": "bun test --watch scripts/",
		"test:unit:logic:coverage": "bun test --coverage scripts/",
		"test:unit:components": "vitest run --config=vitest.unit.config.ts",
		"test:unit:components:watch": "vitest --watch --config=vitest.unit.config.ts",
		"test:unit:components:coverage": "vitest run --coverage --config=vitest.unit.config.ts",
		"test:integration": "vitest run --config=vitest.integration.config.ts",
		"test:integration:watch": "vitest --watch --config=vitest.integration.config.ts",
		"test:integration:coverage": "vitest run --coverage --config=vitest.integration.config.ts",
		"test:all": "bun run test:unit:logic && bun run test:unit:components && bun run test:integration && bun run test:e2e",
		"test:watch": "vitest --watch",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:ci": "bun run test:unit:logic && bun run test:unit:components && bun run test:integration:ci",
		"test:integration:ci": "vitest run --config vitest.integration.config.ts --exclude=\"**/*letta*.integration.test.*\" --exclude=\"**/health/route.test.*\" --exclude=\"**/health/liveness/route.test.*\" --exclude=\"**/version/route.test.*\"",
		"test:coverage": "bun run test:unit:logic:coverage && bun run test:unit:components:coverage && bun run test:integration:coverage",
		"test:bun": "bun test",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:headed": "playwright test --headed",
		"validate-env": "node scripts/validate-build-env.js",
		"verify-deployment": "node scripts/verify-deployment.js pre",
		"verify-deployment:post": "node scripts/verify-deployment.js post",
		"audit": "npm audit --audit-level=moderate",
		"analyze": "ANALYZE=true bun run build",
		"bundle-report": "bun run build && npx webpack-bundle-analyzer .next/static/chunks/*.js",
		"perf": "bun run analyze && bun run bundle-report",
		"pages:build": "npx @cloudflare/next-on-pages",
		"pages:dev": "npx @cloudflare/next-on-pages --dev",
		"deploy": "bun run pages:build && wrangler pages deploy",
		"deploy:pre": "bun run validate-env && bun run build:production && bun run verify-deployment",
		"deploy:post": "bun run verify-deployment:post",
		"deploy:cloudflare": "bun run deploy:pre && bun run pages:build && wrangler pages deploy && bun run deploy:post",
		"deploy:railway": "bun run deploy:pre && railway up && bun run deploy:post",
		"deploy:vercel": "bun run deploy:pre && vercel --prod && bun run deploy:post",
		"cf:dev": "bun run pages:dev",
		"cf:preview": "bun run pages:build && wrangler pages dev",
		"clean": "rm -rf .next .turbo coverage bundle-stats.json bundle-analyzer-report.html",
		"health-check": "curl -f http://localhost:3001/api/health || exit 1",
		"ready-check": "curl -f http://localhost:3001/api/ready || exit 1",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
