{
	"name": "web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack --port=3001",
		"build": "next build",
		"build:production": "NODE_ENV=production next build",
		"build:analyze": "ANALYZE=true next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"check": "tsc --noEmit",
		"typecheck": "tsc --noEmit",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:ci": "vitest run --reporter=verbose --coverage=false",
		"test:coverage": "vitest run --coverage",
		"test:bun": "bun test",
		"validate-env": "node scripts/validate-build-env.js",
		"verify-deployment": "node scripts/verify-deployment.js pre",
		"verify-deployment:post": "node scripts/verify-deployment.js post",
		"audit": "npm audit --audit-level=moderate",
		"analyze": "ANALYZE=true npm run build",
		"bundle-report": "npm run build && npx webpack-bundle-analyzer .next/static/chunks/*.js",
		"perf": "npm run analyze && npm run bundle-report",
		"pages:build": "npx @cloudflare/next-on-pages",
		"pages:dev": "npx @cloudflare/next-on-pages --dev",
		"deploy": "npm run pages:build && wrangler pages deploy",
		"deploy:pre": "npm run validate-env && npm run build:production && npm run verify-deployment",
		"deploy:post": "npm run verify-deployment:post",
		"deploy:cloudflare": "npm run deploy:pre && npm run pages:build && wrangler pages deploy && npm run deploy:post",
		"deploy:railway": "npm run deploy:pre && railway up && npm run deploy:post",
		"deploy:vercel": "npm run deploy:pre && vercel --prod && npm run deploy:post",
		"cf:dev": "npm run pages:dev",
		"cf:preview": "npm run pages:build && wrangler pages dev",
		"clean": "rm -rf .next .turbo coverage bundle-stats.json bundle-analyzer-report.html",
		"health-check": "curl -f http://localhost:3001/api/health || exit 1",
		"ready-check": "curl -f http://localhost:3001/api/ready || exit 1"
	},
	"dependencies": {
		"@ai-sdk/openai": "^1.3.22",
