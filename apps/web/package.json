{
	"name": "web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack --port=3001",
		"build": "next build",
		"build:production": "NODE_ENV=production next build",
		"build:analyze": "ANALYZE=true next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"check": "tsc --noEmit",
		"check-types": "tsc --noEmit",
		"typecheck": "tsc --noEmit",
		"test": "npm run test:unit",
		"test:unit": "npm run test:unit:logic && npm run test:unit:components",
		"test:unit:logic": "bun test scripts/",
		"test:unit:logic:watch": "bun test --watch scripts/",
		"test:unit:logic:coverage": "bun test --coverage scripts/",
		"test:unit:components": "vitest run --config=vitest.unit.config.ts",
		"test:unit:components:watch": "vitest --watch --config=vitest.unit.config.ts",
		"test:unit:components:coverage": "vitest run --coverage --config=vitest.unit.config.ts",
		"test:integration": "vitest run --config=vitest.integration.config.ts",
		"test:integration:watch": "vitest --watch --config=vitest.integration.config.ts",
		"test:integration:coverage": "vitest run --coverage --config=vitest.integration.config.ts",
		"test:all": "npm run test:unit:logic && npm run test:unit:components && npm run test:integration && npm run test:e2e",
		"test:watch": "vitest --watch",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:ci": "npm run test:all",
		"test:coverage": "npm run test:unit:logic:coverage && npm run test:unit:components:coverage && npm run test:integration:coverage",
		"test:bun": "bun test",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:headed": "playwright test --headed",
		"validate-env": "node scripts/validate-build-env.js",
		"verify-deployment": "node scripts/verify-deployment.js pre",
		"verify-deployment:post": "node scripts/verify-deployment.js post",
		"audit": "npm audit --audit-level=moderate",
		"analyze": "ANALYZE=true npm run build",
		"bundle-report": "npm run build && npx webpack-bundle-analyzer .next/static/chunks/*.js",
		"perf": "npm run analyze && npm run bundle-report",
		"pages:build": "npx @cloudflare/next-on-pages",
		"pages:dev": "npx @cloudflare/next-on-pages --dev",
		"deploy": "npm run pages:build && wrangler pages deploy",
		"deploy:pre": "npm run validate-env && npm run build:production && npm run verify-deployment",
		"deploy:post": "npm run verify-deployment:post",
		"deploy:cloudflare": "npm run deploy:pre && npm run pages:build && wrangler pages deploy && npm run deploy:post",
		"deploy:railway": "npm run deploy:pre && railway up && npm run deploy:post",
		"deploy:vercel": "npm run deploy:pre && vercel --prod && npm run deploy:post",
		"cf:dev": "npm run pages:dev",
		"cf:preview": "npm run pages:build && wrangler pages dev",
		"clean": "rm -rf .next .turbo coverage bundle-stats.json bundle-analyzer-report.html",
		"health-check": "curl -f http://localhost:3001/api/health || exit 1",
		"ready-check": "curl -f http://localhost:3001/api/ready || exit 1",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
